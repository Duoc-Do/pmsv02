@model ICollection<AuthenticationClientData>

@if (Model.Count == 0)
{
}
else
{
    using (Html.BeginForm("ExternalLogin", "Account", new { ReturnUrl = ViewBag.ReturnUrl }))
    {
    @Html.AntiForgeryToken()
    <div id="socialLoginList">
        @foreach (AuthenticationClientData p in Model)
        {
            <button class="btn btn-block btn-social btn-facebook btn-primary btn-lg" type="submit" name="provider" value="@p.AuthenticationClient.ProviderName" title="Đăng nhập với @p.DisplayName "><i class="fa fa-facebook"></i> @p.DisplayName</button>

        }

    </div>
    }
}
