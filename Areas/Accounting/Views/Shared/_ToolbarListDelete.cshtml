@model Dictionary<string, string>
@{

    if (!Model.ContainsKey("cancel-close-action"))
    {
        Model.Add("cancel-close-action", "Index");
    }
    if (!Model.ContainsKey("delete-close-action"))
    {
        Model.Add("delete-close-action", "Index");
    }

    if (Model["action-return"] == null)
    { Model["action-return"] = "Index"; }

    string ajaxstring = string.Format("data-ajax-update=#{0} ", Model["ajaxupdateid"]);
    ajaxstring += "data-ajax-mode=replace ";
    ajaxstring += "data-ajax-method=GET ";
    ajaxstring += string.Format("data-ajax-loading=#{0} ", Model["ajaxloadingid"]);
    ajaxstring += "data-ajax=true ";

}
<div class="" role="toolbar">
    <!-- Split button -->
    <div class="btn-group">
        <input id="sv-toolbar-delete-action-return" name="action_return" type="hidden" value="@Model["action-return"]" />
        <a id="sv-toolbar-delete" class="btn btn-primary" onclick="sv_toolbar_delete_submit_delete(this);">
            <span class="fa fa-trash-o"></span>&nbsp;Xóa
        </a>
        @*<a class="btn" href="@Url.Action("Index", new {area="Accounting"})" @(ajaxstring) ><span class="fa fa-long-arrow-left"></span>&nbsp;Trở lại danh sách</a>*@
    </div>
</div>
<script type="text/javascript">
    function sv_toolbar_delete_submit_delete(a) {
        $(a).closest('div').find("#sv-toolbar-delete-action-return").val("@Model["action-return"]");
        $(a).closest('form').submit();
        $("#senvietpopup").modal("hide");
    };
</script>
