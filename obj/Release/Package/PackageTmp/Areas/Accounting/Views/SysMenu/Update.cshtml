@model WebApp.Areas.Accounting.Models.SysMenu
@using WebApp.Areas.Accounting.Services
@{
    Dictionary<string, string> ajaxoption = (Dictionary<string, string>)ViewBag.ajaxoption;
}
<section id="@(ajaxoption["ajaxupdateid"])">
    <div class="box box-primary">
        @Html.Partial("_HeaderEdit")

        @using (@Ajax.BeginForm(new AjaxOptions { LoadingElementId = ajaxoption["ajaxloadingid"], InsertionMode = InsertionMode.Replace, UpdateTargetId = ajaxoption["ajaxupdateid"] }))
        {   
            <div class="box-body">
                @Html.AntiForgeryToken()
                @Html.bsValidationSummary(false)
                <div class="panel panel-default">
                    <div class="panel-body">
@Html.bsEditorFor(model => model.ParentID)@Html.bsEditorFor(model => model.MenuID)@Html.bsEditorFor(model => model.MenuType)@Html.bsEditorFor(model => model.MenuOrder)@*@Html.bsEditorFor(model => model.MenuIcon)*@@Html.bsEditorFor(model => model.Des)@Html.bsEditorFor(model => model.BusinessCode)
@Html.bsEditorFor(model => model.IsActive)
@Html.bsEditorFor(model => model.Note)                    </div>
                </div>

                @Html.Partial("_ToolbarListUpdate", ajaxoption)
            </div>
        }
        @Html.Partial("_FooterEdit")
    </div>
</section>
@Scripts.Render("~/bundles/accapp")