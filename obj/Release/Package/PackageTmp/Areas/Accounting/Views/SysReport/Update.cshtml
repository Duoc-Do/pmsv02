@model WebApp.Areas.Accounting.Models.SysReport
@using WebApp.Areas.Accounting.Services
@{
    Dictionary<string, string> ajaxoption = (Dictionary<string, string>)ViewBag.ajaxoption;
}
<section id="@(ajaxoption["ajaxupdateid"])">
    <div class="box box-primary">
        @Html.Partial("_HeaderEdit")

        @using (@Ajax.BeginForm(new AjaxOptions { LoadingElementId = ajaxoption["ajaxloadingid"], InsertionMode = InsertionMode.Replace, UpdateTargetId = ajaxoption["ajaxupdateid"] }))
        {   
            <div class="box-body">
                @Html.AntiForgeryToken()
                @Html.bsValidationSummary(false)

                <div class="panel panel-default">
                    <div class="panel-body">
@Html.svHiddenFor(model => model.SysReportID)@Html.bsEditorFor(model => model.Title)@Html.bsEditorFor(model => model.TableName)@Html.bsEditorFor(model => model.ReportPath)@Html.bsEditorFor(model => model.ReportName)@Html.bsEditorFor(model => model.ProcedureName)@Html.bsEditorFor(model => model.IsDefault)@Html.bsEditorFor(model => model.Des)@Html.bsEditorFor(model => model.BusinessCode)

                    </div>
                </div>

                @Html.Partial("_ToolbarListUpdate", ajaxoption)
            </div>
        }
        @Html.Partial("_FooterEdit")
    </div>
</section>
@Scripts.Render("~/bundles/accapp")